  "reinforce_mlp_cartpole": {
    "agent": [{
      "name": "Reinforce",
      "algorithm": {
        "name": "Reinforce",
        "action_pdtype": "default",
        "action_policy": "default",
        "explore_var_spec": null,
        "gamma": 0.99,
        "add_entropy": true,
        "entropy_coef_spec": {
          "name": "linear_decay",
          "clock_unit": "total_t",
          "start_val": 0.01,
          "end_val": 0.0001,
          "start_step": 2500,
          "end_step": 10000,
        },
        "training_frequency": 1,
        "normalize_state": true
      },
      "memory": {
        "name": "OnPolicyReplay"
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [64],
        "hid_layers_activation": "relu",
        "clip_grad_val": null,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "Adam",
          "lr": 0.02
        },
        "lr_scheduler_spec": {
          "name": "StepLR",
          "step_size": 400,
          "gamma": 0.9,
        },
        "gpu": false
      }
    }],
    "env": [{
      "name": "CartPole-v0",
      "max_timestep": null,
      "max_epi": 200,
      "save_epi_frequency": 300
    }],
    "body": {
      "product": "outer",
      "num": 1
    },
    "meta": {
      "distributed": false,
      "max_session": 4,
      "max_trial": 95,
      "search": "RandomSearch",
      "resources": {
        "num_cpus": 95
      }
    },
    "search": {
      "agent": [{
        "algorithm": {
          "gamma__uniform": [0.95, 0.99],
          "entropy_coef_spec": {
            "start_val__uniform": [0.001, 0.1],
          }
        },
        "net": {
          "hid_layers_activation__choice": ["tanh", "relu", "selu"],
