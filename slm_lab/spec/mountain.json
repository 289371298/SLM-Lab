{
  "a2c_gae_mlp_separate_mountain": {
    "agent": [{
      "name": "A2C",
      "algorithm": {
        "name": "ActorCritic",
        "action_pdtype": "default",
        "action_policy": "default",
        "action_policy_update": "no_update",
        "explore_var_start": null,
        "explore_var_end": null,
        "explore_anneal_epi": null,
        "gamma": 0.993,
        "use_gae": true,
        "lam": 0.92,
        "use_nstep": false,
        "num_step_returns": 1,
        "add_entropy": true,
        "entropy_coef_start": 0.03,
        "entropy_coef_end": 0.001,
        "entropy_anneal_epi": 300,
        "entropy_anneal_start_epi": 400,
        "policy_loss_coef": 1.0,
        "val_loss_coef": 1.0,
        "training_frequency": 3,
        "training_epoch": 8,
        "normalize_state": true
      },
      "memory": {
        "name": "OnPolicyReplay"
      },
      "net": {
        "type": "MLPNet",
        "shared": false,
        "hid_layers": [400, 200],
        "hid_layers_activation": "relu",
        "clip_grad": false,
        "clip_grad_val": 10.0,
        "use_same_optim": true,
        "actor_optim_spec": {
          "name": "Adam",
          "lr": 0.008,
        },
        "critic_optim_spec": {
          "name": "Adam",
          "lr": 0.008,
        },
        "lr_decay": "linear_decay",
        "lr_decay_frequency": 10000,
        "lr_decay_min_timestep": 10000,
        "lr_anneal_timestep": 250000,
        "gpu": true
      }
    }],
    "env": [{
      "name": "MountainCar-v0",
      "max_timestep": null,
      "max_episode": 3000,
      "save_epi_frequency": 3000,
    }],
    "body": {
      "product": "outer",
      "num": 1
    },
    "meta": {
      "distributed": false,
      "max_session": 4,
      "max_trial": 47,
      "search": "RandomSearch",
      "resources": {
        "num_cpus": 47
      }
    },
    "search": {
      "agent": [{
        "algorithm": {
          "lam__uniform": [0.9, 0.99],
          "gamma__uniform": [0.99, 0.999],
          "training_frequency__choice": [2, 3, 4],
          "training_batch_epoch_choice": [4, 6, 8, 10],
          "entropy_coef_start__uniform": [0.001, 0.05],
        },
        "net": {
          "actor_optim_spec": {
            "lr__uniform": [0.0005, 0.01]
          }
        }
      }]
    }
  },
