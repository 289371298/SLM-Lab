{
  "ppo_unity3dball": {
    "agent": [{
      "name": "PPO",
      "algorithm": {
        "name": "PPO",
        "action_pdtype": "default",
        "action_policy": "default",
        "explore_var_spec": null,
        "gamma": 0.99,
        "lam": 0.95,
        "clip_eps_spec": {
          "name": "no_decay",
          "start_val": 0.20,
          "end_val": 0.20,
          "start_step": 0,
          "end_step": 0
        },
        "entropy_coef_spec": {
          "name": "no_decay",
          "start_val": 0.0,
          "end_val": 0.0,
          "start_step": 0,
          "end_step": 0
        },
        "val_loss_coef": 1.0,
        "time_horizon": 128,
        "minibatch_size": 128,
        "training_epoch": 10
      },
      "memory": {
        "name": "OnPolicyBatchReplay",
      },
      "net": {
        "type": "MLPNet",
        "shared": false,
        "hid_layers": [256, 256],
        "hid_layers_activation": "relu",
        "init_fn": "orthogonal_",
        "clip_grad_val": 0.5,
        "use_same_optim": false,
        "loss_spec": {
          "name": "MSELoss"
        },
        "actor_optim_spec": {
          "name": "Adam",
          "lr": 1e-3,
        },
        "critic_optim_spec": {
          "name": "Adam",
          "lr": 1e-3,
        },
        "lr_scheduler_spec": null,
        "gpu": false
      }
    }],
    "env": [{
      "name": "Unity3DBall-v0",
      "num_envs": 1,
      "max_t": 1000,
      "max_frame": 1e5
    }],
    "body": {
      "product": "outer",
      "num": 1
    },
    "meta": {
      "distributed": false,
      "log_frequency": 500,
      "eval_frequency": 500,
      "rigorous_eval": 0,
      "max_session": 1,
      "max_trial": 1
    }
  },
