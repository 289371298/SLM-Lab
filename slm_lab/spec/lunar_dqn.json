{
  "dqn_boltzmann_poly_lunar": {
    "agent": [{
      "name": "DQN",
      "algorithm": {
        "name": "DQN",
        "action_pdtype": "Argmax",
        "action_policy": "boltzmann",
        "action_policy_update": "linear_decay",
        "explore_var_start": 5.0,
        "explore_var_end": 0.5,
        "explore_anneal_epi": 100,
        "gamma": 0.99,
        "training_batch_epoch": 3,
        "training_epoch": 4,
        "training_frequency": 4,
        "training_min_timestep": 32,
        "normalize_state": true
      },
      "memory": {
        "name": "Replay",
        "batch_size": 32,
        "max_size": 100000,
        "use_cer": true,
        "concat_len": 4
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [400, 200],
        "hid_layers_activation": "relu",
        "clip_grad_val": null,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "Adam",
          "lr": 0.008,
        },
        "lr_scheduler_spec": {
          "name": "MultiStepLR",
          "milestones": [80000],
          "gamma": 0.1,
        },
        "update_type": "polyak",
        "update_frequency": 200,
        "polyak_coef": 0.9,
        "gpu": false
      }
    }],
    "env": [{
      "name": "LunarLander-v2",
      "max_timestep": null,
      "max_episode": 1200,
      "save_epi_frequency": 1200,
    }],
    "body": {
      "product": "outer",
      "num": 1
    },
    "meta": {
      "distributed": false,
      "max_session": 4,
      "max_trial": 128,
      "search": "RandomSearch",
      "resources": {
        "num_cpus": 95,
      }
    },
    "search": {
      "agent": [{
        "algorithm": {
          "normalize_state__choice": [true, false],
          "explore_anneal_epi__choice": [100, 150, 200, 250],
          "gamma__choice": [0.99, 0.993, 0.995],
        },
        "memory": {
          "name": ["Replay", "ConcatReplay"],
        },
        "net": {
          "training_batch_epoch_choice": [1, 2, 3, 4],
          "training_epoch_choice": [1, 2, 3, 4],
          "polyak_coef__choice": [0, 0.9, 0.95, 0.99, 0.995, 0.999],
          "lr_scheduler_spec": {
            "milestones": [
              [50000],
              [60000],
              [70000],
              [800000],
              [50000, 100000],
              [60000, 80000],
              [60000, 100000],
              [70000, 90000],
              [70000, 110000],
              [80000],
              [80000, 100000],
              [80000, 120000],
            ],
          },
          "optim_spec": {
            "lr__uniform": [0.001, 0.01]
          }
        }
      }]
    }
  },
}
