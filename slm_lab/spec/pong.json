{
  "dqn_pong": {
    "agent": [{
      "name": "DQN",
      "algorithm": {
        "name": "DQN",
        "action_pdtype": "Argmax",
        "action_policy": "epsilon_greedy",
        "action_policy_update": "linear_decay",
        "explore_var_start": 1.0,
        "explore_var_end": 0.05,
        "explore_anneal_epi": 200000,
        "gamma": 0.99,
        "training_batch_epoch": 1,
        "training_epoch": 1,
        "training_frequency": 4,
        "training_min_timestep": 10000,
        "normalize_state": false
      },
      "memory": {
        "name": "AtariReplay",
        "batch_size": 32,
        "max_size": 100000,
        "stack_len": 4,
        "use_cer": false
      },
      "net": {
        "type": "ConvNet",
        "conv_hid_layers": [
          [32, 8, 4, 0, 1],
          [64, 4, 2, 0, 1],
          [64, 3, 1, 0, 1]
        ],
        "fc_hid_layers": [512],
        "hid_layers_activation": "relu",
        "init_fn": "kaiming_uniform_",
        "batch_norm": false,
        "clip_grad_val": 0.5,
        "loss_spec": {
          "name": "SmoothL1Loss",
        },
        "optim_spec": {
          "name": "Adam",
          "lr": 1e-3,
        },
        "lr_scheduler_spec": null,
        "update_type": "replace",
        "update_frequency": 1000,
        "gpu": true
      }
    }],
    "env": [{
      "name": "PongNoFrameskip-v4",
      "max_timestep": null,
      "max_episode": 2000,
      "save_epi_frequency": 100
    }],
    "body": {
      "product": "outer",
      "num": 1
    },
    "meta": {
      "distributed": false,
      "graph_x": "total_t",
      "max_session": 1,
      "max_trial": 1,
      "search": "RandomSearch"
    }
  },
  "a3c_conv_separate_cartpole": {
    "agent": [{
      "name": "A3C",
      "algorithm": {
        "name": "ActorCritic",
        "action_pdtype": "default",
        "action_policy": "default",
        "action_policy_update": "no_update",
        "explore_var_start": null,
        "explore_var_end": null,
        "explore_anneal_epi": null,
        "gamma": 0.99,
        "use_gae": true,
        "lam": 0.97,
        "use_nstep": false,
        "num_step_returns": 1,
        "add_entropy": true,
        "entropy_coef_start": 0.01,
        "entropy_coef_end": 0.001,
        "entropy_anneal_epi": 2000,
        "entropy_anneal_start_epi": 1000,
        "policy_loss_coef": 1.0,
        "val_loss_coef": 0.5,
        "training_frequency": 1,
        "training_epoch": 1,
        "normalize_state": false
      },
      "memory": {
        "name": "OnPolicyAtariReplay",
        "stack_len": 4
      },
      "net": {
        "type": "ConvNet",
        "conv_hid_layers": [
          [32, 8, 4, 0, 1],
          [64, 4, 2, 0, 1],
          [64, 3, 1, 0, 1]
        ],
        "fc_hid_layers": [512],
        "hid_layers_activation": "relu",
        "init_fn": "kaiming_uniform_",
        "batch_norm": false,
        "clip_grad_val": 0.5,
        "loss_spec": {
          "name": "SmoothL1Loss",
        },
        "use_same_optim": false,
        "actor_optim_spec": {
          "name": "Adam",
          "lr": 1e-3
        },
        "critic_optim_spec": {
          "name": "Adam",
          "lr": 1e-4
        },
        "lr_scheduler_spec": null,
        "gpu": true
      }
    }],
    "env": [{
      "name": "PongNoFrameskip-v4",
      "max_timestep": null,
      "max_episode": 2000,
      "save_epi_frequency": 100
    }],
    "body": {
      "product": "outer",
      "num": 1
    },
    "meta": {
      "distributed": true,
      "graph_x": "total_t",
      "max_session": 4,
      "max_trial": 1,
      "search": "RandomSearch"
    }
  },
}
